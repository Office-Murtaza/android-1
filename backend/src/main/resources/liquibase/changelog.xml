    <include file="liquibase/create/create_open_hour.sql" relativeToChangelogFile="false"/>
    <include file="liquibase/create/create_link.sql" relativeToChangelogFile="false"/>
    <include file="liquibase/create/create_update_phone.sql" relativeToChangelogFile="false"/>
    
    <include file="liquibase/insert/insert_coins.sql" relativeToChangelogFile="false"/>
